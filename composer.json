{
    "name": "brotkrueml/typo3-prevent-lang-packs-download",
    "description": "Prevent language packs download for configurable TYPO3 extension keys",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/typo3-prevent-lang-packs-download/issues",
        "source": "https://github.com/brotkrueml/typo3-prevent-lang-packs-download",
        "docs": "https://docs.typo3.org/p/brotkrueml/typo3-prevent-lang-packs-download/main/en-us/"
    },
    "require": {
        "php": ">=8.1",
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-install": "^12.4 || ^13.4"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~6.1.0",
        "ergebnis/composer-normalize": "~2.44.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "1.12.11",
        "phpunit/phpunit": "^10.5 || ^11.4",
        "rector/rector": "1.2.10",
        "symfony/yaml": "^6.4 || ^7.1",
        "symplify/phpstan-rules": "^13.0",
        "tomasvotruba/cognitive-complexity": "^0.2.3"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\PreventLangPacksDownload\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\PreventLangPacksDownload\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "prevent_lang_packs_download",
            "web-dir": ".Build/web"
        }
    }
}
